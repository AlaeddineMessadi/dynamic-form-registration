{
  "name": "form-registration-homeht",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "formik": "^2.2.6",
    "lodash": "^4.17.20",
    "node-sass": "4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently \"yarn start\" \"yarn json-server\"",
    "test": "yarn cypress:run",
    "json-server": "json-server --watch db.json --port 3001",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "cypress": "cypress open",
    "cypress:start:app": "BROWSER=none react-scripts -r @cypress/instrument-cra start",
    "cypress:start:wait": "start-server-and-test cypress:start:app http://localhost:3000",
    "cypress:open": "npm run cypress:start:wait -- \"cypress open\"",
    "cypress:run": "npm run cypress:start:wait -- \"cypress run\"",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.8",
    "@cypress/instrument-cra": "^1.4.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "concurrently": "^5.3.0",
    "cypress": "^6.2.0",
    "json-server": "^0.16.3",
    "start-server-and-test": "^1.11.6"
  },
  "proxy": "http://localhost:5000"
}
